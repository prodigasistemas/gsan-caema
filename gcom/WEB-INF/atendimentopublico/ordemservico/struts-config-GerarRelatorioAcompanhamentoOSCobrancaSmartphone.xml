		</action>
		
		<action name="GerarRelatorioAcompanhamentoOSCobrancaSmartphoneActionForm"
			type="gcom.gui.mobile.GerarRelatorioAcompanhamentoOSCobrancaSmartphoneAction"
			input="/exibirGerarRelatorioAcompanhamentoOSCobrancaSmartphoneAction.do"
